{
    "coc.preferences.formatOnSaveFiletypes": ["javascript", "javascriptreact", "typescript", "typescriptreact", "typescript.tsx"],
    "diagnostic.errorSign": "✖",
    "diagnostic.hintSign": "○",
    "diagnostic.infoSign": "●",
    "diagnostic.warningSign": "⚠",
    "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact", "typescript.tsx"],
    "prettier.onlyUseLocalVersion": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.useTabs": true,
    "prettier.printWidth": 120,
    "prettier.arrowParens": "always",
    "prettier.braketSpacing": "true",
    "suggest.maxCompleteItemCount": 20
}
